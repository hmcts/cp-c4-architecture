views {

  view index {
    title 'System Landscape'
    include
      *

    style cp {
      size large
    }
  }

  view common-platform of cp {
    include
      cp,
      cp.court-hearings-subdomain.*,
      cp.scheduling-and-listing-subdomain.*,
      cp.case-administration-subdomain.*,
      cp.opami-subdomain.*,
      cp.case-ingestion-subdomain.*

    style cp {
      opacity 0%
    }
  }

    view hearing-system-context of cp.hearing-system {
      title '[System Context] Hearing System'
      include *
        where
          not (kind is application or kind is database or kind is webapp)
      include cp.court-hearings-subdomain._
      include cp.scheduling-and-listing-subdomain._
      include cp.case-administration-subdomain._
      include cp.opami-subdomain._
      exclude * -> *
      include * <-> cp.hearing-system

      style cp.case-administration-subdomain.*, cp.scheduling-and-listing-subdomain.*, cp.opami-subdomain.* {
        color secondary
      }
    }

    view results-distribution-system-context of cp.results-distribution-system {
      title '[System Context] Results Distribution System'
      include *
    }

     /**
      * @likec4-generated(v1)
      * 
      * 
      * 
      * 
      * 
      * zQHKpmxjMXI4N4OiY3CRgqF4y0CIPAAAAAAAoXnLQIYjKr/LLrahbISheM0DX6F5zQKcpXdpZHRozNymaGVpZ2h0EqFwlJLNAdfNBIuSy0CGHszMzMzNy0CN9ZmZmZmakstAj/0zMzMzM8tAhXJmZmZmZpLLQJOyAAAAAADNAeI=
      */
     view scheduling-and-listing-system-context of cp.scheduling-and-listing-system {
      title '[System Context] Scheduling and Listing System'
      include *
        where
          not (kind is application or kind is database or kind is webapp)
      include cp.court-hearings-subdomain._
      include cp.case-administration-subdomain._
      include cp.opami-subdomain._
      exclude * -> *
      include * <-> cp.scheduling-and-listing-system

      style cp.case-administration-subdomain.*, cp.court-hearings-subdomain.*, cp.opami-subdomain.*, cp.court-hearings-subdomain.results-distribution-system {
        color secondary
      }
  	// autoLayout TopBottom
	}
	

    view scheduling-and-listing-system-container-context of cp.scheduling-and-listing-system {
      title '[Container Context] Scheduling and Listing System'
      include
        *
      // include cp.court-hearings-subdomain.hearing-system
      include cp.court-hearings-subdomain.hearing-system.staging-darts.*
      // include * <-> cp.scheduling-and-listing-subdomain.scheduling-and-listing-system.*
    }
}
