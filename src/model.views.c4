views {

  view index {
    title 'System Landscape'
    include
      *

    style cp {
      size large
    }
  }

  view common-platform of cp {
    title 'Common Platform'
    include
      cp,
      cp.court-hearings-subdomain.*,
      cp.scheduling-and-listing-subdomain,
      cp.scheduling-and-listing-subdomain.*,
      cp.case-administration-subdomain.*,
      cp.opami-subdomain,
      cp.opami-subdomain.*,
      cp.case-ingestion-subdomain.*,
      cp.shared-components-subdomain
    exclude * <-> *

    style cp {
      opacity 0%
    }
  }

    view hearing-system-context of cp.hearing-system {
      title '[System Context] Hearing System'
      include *
        where
          not (kind is application or kind is database or kind is webapp)
      include cp.court-hearings-subdomain._
      include cp.scheduling-and-listing-subdomain._
      include cp.case-administration-subdomain._
      include cp.opami-subdomain._
      include cp.case-ingestion-subdomain._
      exclude * -> *
      include * <-> cp.hearing-system

      style cp.case-administration-subdomain.*, cp.scheduling-and-listing-subdomain.*, cp.opami-subdomain.*, cp.case-ingestion-subdomain.* {
        color secondary
      }
    }

    view results-distribution-system-context of cp.results-distribution-system {
      title '[System Context] Results Distribution System'
      include *
    }

     /**
      * @likec4-generated(v1)
      * 
      * 
      * 
      * 
      * 
      * Y3CRgqF4y0CIPAAAAAAAoXnLQIYjKr/LLrahbISheM0DX6F5zQKcpXdpZHRozNymaGVpZ2h0EqFwlJLNAdfNBIuSy0CGHszMzMzNy0CN9ZmZmZmakstAj/0zMzMzM8tAhXJmZmZmZpLLQJOyAAAAAADNAeI=
      */
     view scheduling-and-listing-system-context of cp.scheduling-and-listing-system {
      title '[System Context] Scheduling and Listing System'
      include *
        where
          not (kind is application or kind is database or kind is webapp)
      include cp.court-hearings-subdomain._
      include cp.case-administration-subdomain._
      include cp.opami-subdomain._
      exclude * -> *
      include * <-> cp.scheduling-and-listing-system

      style cp.case-administration-subdomain.*, cp.court-hearings-subdomain.*, cp.opami-subdomain.*, cp.court-hearings-subdomain.results-distribution-system {
        color secondary
      }
	}
	

    view scheduling-and-listing-system-container-view of cp.scheduling-and-listing-system {
      title '[Container View] Scheduling and Listing System'
      include
        *
      // include cp.court-hearings-subdomain.hearing-system
      include cp.court-hearings-subdomain.hearing-system.staging-darts.*
      // include * <-> cp.scheduling-and-listing-subdomain.scheduling-and-listing-system.*
    }

    view court-scheduler-service-component-view of cp.scheduling-and-listing-system.court-scheduler-service {
      title '[Component View] Court Scheduler Service'
      include
        *
    }

    view spi-system-context of cp.spi {
      title '[System Context] SPI'
      include *
        where
          not (kind is application or kind is database or kind is webapp)
      include cp.case-administration-subdomain._
      include cp.court-hearings-subdomain._
      exclude * -> *
      include * <-> cp.spi

      style cp.case-administration-subdomain.*, cp.court-hearings-subdomain.* {
        color secondary
      }
	  }

    view spi-container-view of cp.spi {
      title '[Container View] SPI'
      include
        *
      include cp.case-management-system
      include cp.court-hearings-subdomain.hearing-system
      exclude * -> *
      include * <-> cp.spi
      include cp.spi <-> cp.spi

      style cp.case-administration-subdomain.*, cp.court-hearings-subdomain.* {
        color secondary
      }
    }
}
